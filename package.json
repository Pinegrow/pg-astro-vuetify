{
  "name": "pg-astro-vuetify",
  "type": "module",
  "version": "0.0.1",
  "homepage": "https://pg-astro-vuetify.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/pinegrow/pg-astro-vuetify.git"
  },
  "scripts": {
    "dev": "astro dev",
    "now": "npm run build && npm run preview",
    "lighthouse:dev": "npx unlighthouse --site http://localhost:4321",
    "lighthouse": "npx unlighthouse --site https://pg-astro-vuetify.netlify.app",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "typecheck": "vue-tsc --noEmit",
    "vue-devtools": "vue-devtools",
    "lint": "npm run lint:fix && npm run format",
    "lint:fix": "eslint --fix --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts ./ --ignore-path .gitignore",
    "format": "prettier --write \"**/*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts,html,css,md,mdx,json}\" !public --ignore-path .gitignore"
  },
  "dependencies": {
    "@astrojs/mdx": "^3.1.5",
    "@astrojs/preact": "^3.5.2",
    "@astrojs/react": "^3.6.2",
    "@astrojs/rss": "^4.0.7",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/solid-js": "^4.4.1",
    "@astrojs/svelte": "^5.7.0",
    "@astrojs/vue": "^4.5.0",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@vueuse/core": "^11.0.3",
    "@vueuse/head": "^2.0.0",
    "astro": "^4.14.6",
    "defu": "^6.1.4",
    "lite-youtube-embed": "^0.3.3",
    "pinia": "^2.2.2",
    "preact": "^10.23.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sass": "^1.77.8",
    "solid-js": "^1.8.22",
    "svelte": "^4.2.19",
    "vee-validate": "^4.13.2",
    "vue": "^3.4.38",
    "vuetify": "^3.7.1"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.242",
    "@nuxt/eslint-config": "^0.5.3",
    "@pinegrow/astro-module": "^3.0.60",
    "@pinegrow/vuetify-plugin": "^3.0.51",
    "@types/node": "^22.5.1",
    "@unocss/preset-icons": "^0.62.3",
    "@vue/devtools": "^6.6.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "npm-run-all": "^4.1.5",
    "ofetch": "^1.3.4",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "typescript": "^5.5",
    "unocss": "^0.62.3",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-vue-components": "^0.27.4",
    "vite-plugin-vue-devtools": "^7.3.9",
    "vite-plugin-vuetify": "^2.0.4",
    "vue-tsc": "^2.0"
  },
  "web-types": "web-types.json"
}
