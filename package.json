    "@astrojs/sitemap": "^3.0.2",
    "@astrojs/solid-js": "^3.0.2",
    "@astrojs/svelte": "^4.0.3",
    "@astrojs/vue": "^3.0.2",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@vueuse/core": "^10.5.0",
    "@vueuse/head": "^2.0.0",
    "astro": "^3.4.0",
    "defu": "^6.1.3",
    "lite-youtube-embed": "^0.3.0",
    "pinia": "^2.1.7",
    "preact": "^10.18.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.69.5",
    "solid-js": "^1.8.4",
    "svelte": "^4.2.2",
    "vee-validate": "^4.11.8",
    "vue": "^3.3.7",
    "vuetify": "^3.3.23"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.135",
    "@nuxt/eslint-config": "^0.2.0",
    "@pinegrow/astro-module": "^3.0.27",
    "@pinegrow/vuetify-plugin": "^3.0.45",
