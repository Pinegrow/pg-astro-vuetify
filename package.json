    "@unocss/preset-icons": "^0.57.1",
    "@vue/devtools": "^6.5.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "ofetch": "^1.3.3",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.1",
    "typescript": "^5.2",
    "unocss": "^0.57.1",
    "unplugin-auto-import": "^0.16.7",
